Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"FindChordQ", "::", "usage"}], "=", 
   "\"\<FindChordQ[g,el] yields True if directed cycle consisting of el has a \
(pseudo-)chord and False otherwise.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"FindChordQ", "[", 
    RowBox[{"g_Graph", ",", "el_List"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"vl", ",", "vpl", ",", "pl", ",", "chordlist"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"vl", "=", 
       RowBox[{"VertexList", "@", 
        RowBox[{"Subgraph", "[", 
         RowBox[{"g", ",", "el"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"vpl", "=", 
       RowBox[{"Subsets", "[", 
        RowBox[{"vl", ",", 
         RowBox[{"{", "2", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"pl", "=", 
       RowBox[{"Union", "[", 
        RowBox[{
         RowBox[{"DirectedEdge", "@@@", "vpl"}], ",", 
         RowBox[{"Reverse", "/@", 
          RowBox[{"DirectedEdge", "@@@", "vpl"}]}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"chordlist", "=", 
       RowBox[{"Complement", "[", 
        RowBox[{"pl", ",", "el"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "@", 
       RowBox[{"Apply", "[", 
        RowBox[{"Or", ",", 
         RowBox[{
          RowBox[{
           RowBox[{"EdgeQ", "[", 
            RowBox[{"g", ",", "#"}], "]"}], "&"}], "/@", "chordlist"}]}], 
        "]"}]}]}]}], "]"}]}], ";"}]}], "Input",
